<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns="http://www.springframework.org/schema/security"
	xmlns:beans="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<beans:bean id="UsuarioService"
		class="romaneo.unificado.services.UsuarioServiceImple">
		<beans:property name="dao" ref="UsuarioDao" />
	</beans:bean>

	<beans:bean id="AcondicionadorService"
		class="romaneo.unificado.services.acondicionador.AcondicionadorServiceImple">
		<beans:property name="dao" ref="AcondicionadorDao" />
	</beans:bean>

	<beans:bean id="ContactoService"
		class="romaneo.unificado.services.ContactoServiceImple">
		<beans:property name="dao" ref="ContactoDao" />
	</beans:bean>

	<beans:bean id="ProductorService"
		class="romaneo.unificado.services.productor.ProductorServiceImple">
		<beans:property name="dao" ref="ProductorDao" />
	</beans:bean>

	<beans:bean id="EstablecimientoService"
		class="romaneo.unificado.services.establecimiento.EstablecimientoServiceImple">
		<beans:property name="dao" ref="EstablecimientoDao" />
	</beans:bean>

	<beans:bean id="ContratistaService"
		class="romaneo.unificado.services.contratista.ContratistaServiceImple">
		<beans:property name="dao" ref="ContratistaDao" />
	</beans:bean>

	<beans:bean id="LocalidadService"
		class="romaneo.unificado.services.LocalidadServiceImple">
		<beans:property name="dao" ref="LocalidadDao" />
	</beans:bean>

	<beans:bean id="MessageService"
		class="romaneo.unificado.services.MessageServiceImple">
		<beans:property name="dao" ref="MessageDao" />
	</beans:bean>

	<beans:bean id="LoggingService"
		class="romaneo.unificado.services.LoggingServiceImple">
		<beans:property name="dao" ref="LoggingDao" />
	</beans:bean>

</beans:beans>