<?page contentType="text/html;charset=UTF-8"?>
<zk xmlns:w="client" xmlns:n="xhtml">
	<window id="acondicionadorFormWndw" closable="true"
		action="show: slideDown;hide: slideUp">
		<n:div
			apply="romaneo.unificado.controllers.acondicionador.AcondicionadorFormNuevoController">
			<script src="/css/bootstrap/js/bootstrap.js" />
			<style src="/css/bootstrap/css/bootstrap.css" />

			<n:div>
				<!-- Nav tabs -->
				<n:ul class="nav nav-tabs" role="tablist">
					<n:li role="presentation" class="active">
						<n:a href="#general" aria-controls="general"
							role="tab" data-toggle="tab">
							${labels.app.general}
						</n:a>
					</n:li>
					<n:li role="presentation">
						<n:a href="#domicilios"
							aria-controls="domicilios" role="tab" data-toggle="tab">
							${labels.app.domicilios }
						</n:a>
					</n:li>
					<n:li role="presentation">
						<n:a href="#contactos" aria-controls="contactos"
							role="tab" data-toggle="tab">
							${labels.app.contactos }
						</n:a>
					</n:li>

				</n:ul>

				<!-- ............................................ -->
				<!-- ......... TABS ............................. -->
				<!-- ............................................ -->

				<!-- Tab panes -->
				<n:div class="tab-content">

					<!-- ............................................ -->
					<!-- ......... general .......................... -->
					<!-- ............................................ -->
					<n:div role="tabpanel" class="tab-pane active"
						id="general">
						<n:div class="col-md-12">
							<label
								value="${labels.acondicionador.nombre}" />
							<textbox id="nombreTxtbx" maxlength="50"
								hflex="1" class="form-control" />

							<label
								value="${labels.acondicionador.apellido}" />
							<textbox id="apellidoTxtbx" maxlength="50"
								hflex="1" class="form-control" />

							<label value="${labels.acondicionador.dni}" />
							<intbox id="dniIntbx" maxlength="8"
								constraint="no negative" hflex="1" class="form-control" />
						</n:div>
					</n:div>

					<!-- ............................................ -->
					<!-- ......... domicilios ....................... -->
					<!-- ............................................ -->
					<n:div role="tabpanel" class="tab-pane"
						id="domicilios">
						<n:div class="col-md-12">

							<n:span class="pull-right">
								<n:div class="btn-group">
									<button id="newBttnVE"
										iconSclass="z-icon-plus" label="${labels.app.create}"
										class="btn btn-sm btn-primary" />
									<button id="editBttnVE"
										disabled="true" iconSclass="z-icon-edit"
										label="${labels.app.edit}" class="btn btn-sm btn-default" />
									<button id="deleteBttnVE"
										disabled="true" iconSclass="z-icon-times"
										label="${labels.app.delete}" class="btn btn-sm btn-default" />
								</n:div>
							</n:span>

							<label value="Domicilios" />
							<listbox id="domiciliosLstbx" rows="10">
								<listhead>
									<listheader label="DirecciÃ³n"
										hflex="min" align="center" />
								</listhead>
							</listbox>
						</n:div>

					</n:div>

					<!-- ............................................ -->
					<!-- ......... contactos ........................ -->
					<!-- ............................................ -->
					<n:div role="tabpanel" class="tab-pane"
						id="contactos">
						<n:div class="col-md-12">
							<n:span class="pull-right">
								<n:div class="btn-group">
									<button id="newBttnContactos"
										iconSclass="z-icon-plus" label="${labels.app.create}"
										class="btn btn-sm btn-primary" />
									<button id="editBttnContactos"
										disabled="true" iconSclass="z-icon-edit"
										label="${labels.app.edit}" class="btn btn-sm btn-default" />
									<button id="deleteBttnContoctas"
										disabled="true" iconSclass="z-icon-times"
										label="${labels.app.delete}" class="btn btn-sm btn-default" />
								</n:div>
							</n:span>
						</n:div>

						<label value="Telefonos" />
						<listbox id="telefonoLstbx" rows="10">
							<listhead>
								<listheader label="Telefonos"
									hflex="min" align="center" />
							</listhead>
						</listbox>

						<label value="E-mail" />
						<listbox id="emailLstbx " rows="10">
							<listhead>
								<listheader label="E-mail" hflex="min"
									align="center" />
							</listhead>
						</listbox>

					</n:div>

				</n:div>

			</n:div>

			<n:div class="col-md-12">
				<n:span class="pull-right">
					<n:span class="pull-down">
						<n:div class="btn-group">
							<n:button class="btn" id="btnVolverVE">
								Volver
							</n:button>
							<n:button class="btn" id="btnEditarVE">
								Editar
							</n:button>
							<n:button class="btn" id="btnGuardarVE">
								Guardar
							</n:button>
						</n:div>
					</n:span>
				</n:span>
			</n:div>

		</n:div>
	</window>
</zk>